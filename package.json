{
  "name": "coloring-portal",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -r dev",
    "dev:all": "pnpm --parallel --filter web --filter worker dev",
    "lint": "pnpm -r lint",
    "build": "pnpm -r build",
    "postinstall": "pnpm dlx prisma@6.19.2 generate --schema packages/db/prisma/schema.prisma"
  }
}
